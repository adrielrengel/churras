<p id="notice"><%= notice %></p>

<h1>Listing Tipo Contatos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_contatos.each do |tipo_contato| %>
      <tr>
        <td><%= tipo_contato.nome %></td>
        <td><%= tipo_contato.usuario %></td>
        <td><%= link_to 'Show', tipo_contato %></td>
        <td><%= link_to 'Edit', edit_tipo_contato_path(tipo_contato) %></td>
        <td><%= link_to 'Destroy', tipo_contato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo contato', new_tipo_contato_path %>
