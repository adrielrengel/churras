<p id="notice"><%= notice %></p>

<h1>Listing Saida Caixas</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Tipo</th>
      <th>Observacao</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saida_caixas.each do |saida_caixa| %>
      <tr>
        <td><%= saida_caixa.data %></td>
        <td><%= saida_caixa.tipo %></td>
        <td><%= saida_caixa.observacao %></td>
        <td><%= saida_caixa.usuario %></td>
        <td><%= link_to 'Show', saida_caixa %></td>
        <td><%= link_to 'Edit', edit_saida_caixa_path(saida_caixa) %></td>
        <td><%= link_to 'Destroy', saida_caixa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saida caixa', new_saida_caixa_path %>
